{
  "name": "oliver",
  "version": "1.1.1",
  "description": "oliver: An Ionic project",
  "scripts": {
    "devserver": "cp www/webpack-template-hotreload.html www/index.html && webpack-dev-server --content-base www/",
    "watch": "cp www/webpack-template-hotreload.html www/index.html && webpack-dev-server --hot --inline --content-base www/",
    "build-assets": "webpack --config webpack.prod.config.js --progress --colors",
    "build": "./build.sh",
    "ios": "./build.sh && ionic run ios",
    "android": "./build.sh && ionic run android",
    "lint": "eslint --ext .js www/js"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.3.3",
    "clean-webpack-plugin": "^0.1.8",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp-util": "^2.2.14",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "^1.6.3",
    "ionic-sdk": "^1.2.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.8.2",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.2.0",
    "shelljs": "^0.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ]
}